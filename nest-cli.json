{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/nest-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest-app",
  "projects": {
    "nest-app": {
      "type": "application",
      "root": "apps/nest-app",
      "entryFile": "main",
      "sourceRoot": "apps/nest-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-app/tsconfig.app.json"
      }
    },
    "other": {
      "type": "application",
      "root": "apps/other",
      "entryFile": "main",
      "sourceRoot": "apps/other/src",
      "compilerOptions": {
        "tsConfigPath": "apps/other/tsconfig.app.json"
      }
    }
  }
}